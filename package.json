{
  "name": "orbital-sync",
  "version": "1.0.3",
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "install:hooks": "git config core.hooksPath '.hooks'",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "test:e2e": "docker-compose -f docker-compose.test.yml up --build --abort-on-container-exit",
    "preversion": "npm run test",
    "version": "auto-changelog -p && git add CHANGELOG.md && git commit -m 'Update CHANGELOG.md'",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattwebbio/orbital-sync.git"
  },
  "bin": "./dist/index.js",
  "devDependencies": {
    "@tsconfig/node18-strictest-esm": "^1.0.1",
    "@types/jest": "^29.0.0",
    "@types/luxon": "^3.0.1",
    "@types/node": "^18.7.14",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "auto-changelog": "^2.4.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.0.1",
    "nock": "^13.2.9",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@honeybadger-io/js": "^4.1.1",
    "chalk": "^4.1.2",
    "fetch-cookie": "^2.1.0",
    "node-fetch": "^3.2.10",
    "node-html-parser": "^5.4.2",
    "sleep-promise": "^9.1.0"
  }
}
